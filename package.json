    "precommit": "lint-staged && npm run prepublish",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.18.0",
    "eslint-config-prev": "^6.10.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^6.1.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(js)$": "babel-jest"
    }
  },
  "lint-staged": {
    "^.+\\.(js)$": [
      "eslint --fix",
      "git add"
    ]
